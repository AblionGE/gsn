<virtual-sensor name="odbc_Matterhorn_Dozer_Base_Debug" priority="10">
    <processing-class>
        <class-name>gsn.vsensor.BridgeVirtualSensor</class-name>
        <unique-timestamps>false</unique-timestamps>
        <output-structure>
            <field name="GEN_TIME" type="BIGINT"/>
            <field name="HEADER_SEQNR" type="INTEGER"/>
            <field name="HEADER_ORIGINATORID" type="INTEGER"/>
            <field name="HEADER_ATIME_HIGH" type="SMALLINT"/>
            <field name="HEADER_ATIME_LOW" type="INTEGER"/>
            <field name="PAYLOAD_RASTATE" type="SMALLINT"/>
            <field name="PAYLOAD_QUEUE" type="INTEGER"/>
            <field name="PAYLOAD_RADIOSLEEP" type="BIGINT"/>
            <field name="PAYLOAD_RADIOUP" type="BIGINT"/>
        </output-structure>
    </processing-class>
    <description>Dozer messages (DozerBaseDebugMsg) from Matterhorn deployment</description>
    <life-cycle pool-size="10"/>
    <addressing/>
    <storage/>
    <streams>
        <stream name="data">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="jdbc">
                    <predicate key="table-name">permasense.Matterhorn_DozerBaseDebug_3933</predicate>
                    <predicate key="start-time">continue</predicate>
                </address>
                <query>select * from wrapper</query>
            </source>
            <query>select * from source</query>
        </stream>
    </streams>
</virtual-sensor>
