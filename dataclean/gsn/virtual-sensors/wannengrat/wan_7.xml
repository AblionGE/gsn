<virtual-sensor name="WAN7" priority="10" >
    <processing-class>
		<class-name>gsn.vsensor.BridgeVirtualSensor</class-name>
		  <init-params/>
	    <output-structure>
				<field name="record"     type="double" />
				<field name="RH"     type="double" />
				<field name="TA"     type="double" />
				<field name="VW_WVc_1"    type="double" />
				<field name="VW_WVc_2"    type="double" />
				<field name="VW_WVc_3"    type="double" />
				<field name="VW_WVc_4"    type="double" />
				<field name="VW_Max"     type="double" />
				<field name="TSS"     type="double" />
				<field name="HS1"     type="double" />
				<field name="SW_in" type="double" />
				<field name="SW_out" type="double" />
				<field name="LW_in" type="double" />
				<field name="LW_out" type="double" />
	    </output-structure>
    </processing-class>
    <description>Wannengrat weather station 7 in Davos, Switzerland. Copyright owner of the data is SLF.</description>
    <life-cycle pool-size="10" />
    <addressing>
		<predicate key="geographical">Davos, Switzerland.</predicate>
	 	<predicate key="LATITUDE">46.8080777778</predicate>
		<predicate key="LONGITUDE">9.790613888</predicate>
		<predicate key="ALTITUDE">2399</predicate>
		<predicate key="NAME">WAN7</predicate>
		<predicate key="GPS_Percision">10</predicate>
		<predicate key="SLOPE">0</predicate>
		<predicate key="exposition">NULL</predicate>
	</addressing>
	<storage/>
    <streams>
	    <stream name="DATA" >
	         <source alias="source" storage-size="1" sampling-rate="1">
	            <address wrapper="toa5">
	               <predicate key="file">Wan_7.met</predicate>
	               <predicate key="not_a_number">NaN,7999,-7999</predicate>
	               <predicate key="sampling">600</predicate> <!-- 10 mins, the value is in msc.-->
	            </address>
	            <query>select * from wrapper</query>
	         </source>
	         <query>select * from source</query>
	    </stream>
    </streams>
</virtual-sensor> 