<virtual-sensor name="Rooftop_DozerRssi_mapped_1390" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.BridgeVirtualSensorBackLog</class-name>
        <unique-timestamps>false</unique-timestamps>
        <init-params>
            <param name="position_mapping" />
        </init-params>        
        <output-structure>
            <field name="TIMESTAMP" type="BIGINT"/>
            <field name="GENERATION_TIME" type="BIGINT"/>
            <field name="POSITION" type="INTEGER" />
            <field name="HEADER_SEQNR" type="INTEGER" />
            <field name="HEADER_ORIGINATORID" type="INTEGER" />
            <field name="HEADER_ATIME" type="INTEGER" />
            <field name="PAYLOAD_RSSI_ID0" type="INTEGER" />
            <field name="PAYLOAD_RSSI_ID1" type="INTEGER" />
            <field name="PAYLOAD_RSSI_ID2" type="INTEGER" />
            <field name="PAYLOAD_RSSI_ID3" type="INTEGER" />
            <field name="PAYLOAD_RSSI_ID4" type="INTEGER" />
            <field name="PAYLOAD_RSSI_ID5" type="INTEGER" />
            <field name="PAYLOAD_RSSI_ID6" type="INTEGER" />
            <field name="PAYLOAD_RSSI_RSSI0" type="SMALLINT" />
            <field name="PAYLOAD_RSSI_RSSI1" type="SMALLINT" />
            <field name="PAYLOAD_RSSI_RSSI2" type="SMALLINT" />
            <field name="PAYLOAD_RSSI_RSSI3" type="SMALLINT" />
            <field name="PAYLOAD_RSSI_RSSI4" type="SMALLINT" />
            <field name="PAYLOAD_RSSI_RSSI5" type="SMALLINT" />
            <field name="PAYLOAD_RSSI_RSSI6" type="SMALLINT" />
        </output-structure>
    </processing-class>
    <description>Dozer messages (DozerRssiMsg) from ETZ rooftop deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage />
    <streams>
        <stream name="data">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="local">
                    <predicate key="query"> select * from Rooftop_DozerRssi_1376 </predicate>
                </address>
                <query> select *, -1 as POSITION from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
    </streams>
</virtual-sensor>
