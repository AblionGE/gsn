<virtual-sensor name="Rooftop_DozerAdcMux1_3933" priority="10" >
    <processing-class>
       <class-name>gsn.vsensor.BridgeVirtualSensor</class-name>
       <unique-timestamps>false</unique-timestamps>
        <output-structure>
                <field name="GEN_TIME" type="BIGINT"/>
                <field name="HEADER_ATIME_HIGH" type="SMALLINT" />
                <field name="HEADER_ATIME_LOW" type="INTEGER" />
                <field name="HEADER_ORIGINATORID" type="INTEGER" />
                <field name="HEADER_SEQNR" type="INTEGER" />
                <field name="PAYLOAD_SAMPLENO" type="INTEGER" />
                <field name="PAYLOAD_SIBADC00" type="INTEGER" />
                <field name="PAYLOAD_SIBADC01" type="INTEGER" />
                <field name="PAYLOAD_SIBADC02" type="INTEGER" />
                <field name="PAYLOAD_SIBADC03" type="INTEGER" />
                <field name="PAYLOAD_SIBADC04" type="INTEGER" />
                <field name="PAYLOAD_SIBADC05" type="INTEGER" />
                <field name="PAYLOAD_SIBADC06" type="INTEGER" />
                <field name="PAYLOAD_SIBADC07" type="INTEGER" />
                <field name="PAYLOAD_SIBADC08" type="INTEGER" />
                <field name="PAYLOAD_SIBADC09" type="INTEGER" />
