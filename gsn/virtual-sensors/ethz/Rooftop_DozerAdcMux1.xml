    <description>dozer messages (DozerAdcMux1Msg) from ETZ rooftop deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage />
    <streams>
    <stream name="data">
      <source alias="source" storage-size="1" sampling-rate="1">
        <address wrapper="ps-backlog">
          <predicate key="deployment">permasense-etzl-bs01.ethz.ch:9002</predicate>
          <predicate key="plugin-classname">gsn.wrappers.backlog.plugins.MigMessagePlugin</predicate>
          <predicate key="message-classname">ch.ethz.permafrozer.messages.DozerAdcMux1Msg</predicate>
          <predicate key="local-sf-port">9003</predicate>
        </address>
        <query> select * from wrapper </query>
      </source>
      <query> select * from source</query>
    </stream>
  </streams>
</virtual-sensor>
