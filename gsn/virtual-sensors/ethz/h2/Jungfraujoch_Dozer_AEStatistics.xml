<virtual-sensor name="Jungfraujoch_Dozer_AEStatistics" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.BridgeVirtualSensorPermasense</class-name>
        <unique-timestamps>false</unique-timestamps>
        <output-structure>
            <field name="DEVICE_ID" type="INTEGER" />
            <field name="GENERATION_TIME" type="BIGINT" unit="unixtime" index="true"/>
            <field name="TIMESTAMP" type="BIGINT" unit="unixtime"/>
            <field name="HEADER_SEQNR" type="INTEGER" />
            <field name="HEADER_ORIGINATORID" type="INTEGER" />
            <field name="HEADER_ATIME" type="INTEGER" unit="s"/>
            <field name="PAYLOAD_MEASURED" type="INTEGER" />            
            <field name="PAYLOAD_PARAMETRIZED" type="INTEGER" />
            <field name="PAYLOAD_STORED" type="INTEGER" />
            <field name="PAYLOAD_BUFFERUNDERFLOWS" type="INTEGER" />
            <field name="PAYLOAD_CH1_MEASURED" type="INTEGER" />            
            <field name="PAYLOAD_CH1_PARAMETRIZED" type="INTEGER" />
            <field name="PAYLOAD_CH1_STORED" type="INTEGER" />
            <field name="PAYLOAD_CH1_BUFFERUNDERFLOWS" type="INTEGER" />
            <field name="PAYLOAD_CH1_NOISEFLOOR" unit="dB" type="SMALLINT" />
            <field name="PAYLOAD_CH2_MEASURED" type="INTEGER" />            
            <field name="PAYLOAD_CH2_PARAMETRIZED" type="INTEGER" />
            <field name="PAYLOAD_CH2_STORED" type="INTEGER" />
            <field name="PAYLOAD_CH2_BUFFERUNDERFLOWS" type="INTEGER" />
            <field name="PAYLOAD_CH2_NOISEFLOOR" unit="dB" type="SMALLINT" />
            <field name="DATA_IMPORT_SOURCE" type="SMALLINT" />
        </output-structure>
    </processing-class>
    <description>Dozer messages (DozerAEStatisticsMsg) from Jungfraujoch deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage />
        <streams>
        <stream name="data1">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="backlog">
                    <predicate key="remote-connection">permasense-jj-bs01.ethz.ch:9003</predicate>
                    <predicate key="plugin-classname">gsn.wrappers.backlog.plugins.MigMessagePlugin</predicate>
                    <predicate key="message-classname">ch.ethz.permasense.tinyos2x.messages.DozerAEStatisticsMsg</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
        <stream name="dataold1">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="backlog">
                    <predicate key="remote-connection">permasense-jj-bs01.ethz.ch:9003</predicate>
                    <predicate key="plugin-classname">gsn.wrappers.backlog.plugins.MigMessagePlugin</predicate>
                    <predicate key="message-classname">ch.ethz.permasense.tinyos2x.messages.DozerAEStatisticsOldMsg</predicate>
                </address>
                <query> select DEVICE_ID, GENERATION_TIME, TIMESTAMP, HEADER_SEQNR,HEADER_ORIGINATORID,HEADER_ATIME,PAYLOAD_MEASURED,PAYLOAD_PARAMETRIZED,PAYLOAD_STORED,PAYLOAD_BUFFERUNDERFLOWS_CH1 as PAYLOAD_CH1_BUFFERUNDERFLOWS,PAYLOAD_BUFFERUNDERFLOWS_CH2 as PAYLOAD_CH2_BUFFERUNDERFLOWS from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
        

        <stream name="data2">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="backlog">
                    <predicate key="remote-connection">permasense-caa-jj-2.dyndns.biz:9003</predicate>
                    <predicate key="plugin-classname">gsn.wrappers.backlog.plugins.MigMessagePlugin</predicate>
                    <predicate key="message-classname">ch.ethz.permasense.tinyos2x.messages.DozerAEStatisticsMsg</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
        <stream name="dataold2">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="backlog">
                    <predicate key="remote-connection">permasense-caa-jj-2.dyndns.biz:9003</predicate>
                    <predicate key="plugin-classname">gsn.wrappers.backlog.plugins.MigMessagePlugin</predicate>
                    <predicate key="message-classname">ch.ethz.permasense.tinyos2x.messages.DozerAEStatisticsOldMsg</predicate>
                </address>
                <query> select DEVICE_ID, GENERATION_TIME, TIMESTAMP, HEADER_SEQNR,HEADER_ORIGINATORID,HEADER_ATIME,PAYLOAD_MEASURED,PAYLOAD_PARAMETRIZED,PAYLOAD_STORED,PAYLOAD_BUFFERUNDERFLOWS_CH1 as PAYLOAD_CH1_BUFFERUNDERFLOWS,PAYLOAD_BUFFERUNDERFLOWS_CH2 as PAYLOAD_CH2_BUFFERUNDERFLOWS from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
    </streams>
</virtual-sensor>
