<virtual-sensor name="OSTest_GasMobile_Ozone_Uncalibrated" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.GasMobileDataParser</class-name>
        <unique-timestamps>false</unique-timestamps>
        <output-structure>
            <field name="POSITION" type="INTEGER" />
            <field name="DEVICE_ID" type="INTEGER" />
            <field name="GENERATION_TIME" type="BIGINT" unit="unixtime" index="true"/>
		    <field name="OZONE_PPB" type="DOUBLE" unit="ppb" />
            <field name="RESISTANCE" type="INTEGER" unit="kOhm"/>
            <field name="TEMPERATURE" type="DOUBLE" unit="C"/>
            <field name="HUMIDITY" type="INTEGER" unit="%"/>
		    
		    <field name="LATITUDE" type="DOUBLE" unit="N"/>
            <field name="LONGITUDE" type="DOUBLE" unit="E"/>
		    <field name="SPEED" type="DOUBLE" />
		    
            <field name="DATA_IMPORT_SOURCE" type="SMALLINT" />
        </output-structure>
    </processing-class>
    <description>Android GasMobile parsed data from OSTest deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage storage-directory="/home/perma/permasense_vault/gsn-test-binaries/" />
    <streams>
        <stream name="data">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="local">
                    <predicate key="query">select * from OSTest_Binary__mapped</predicate>
                    <predicate key="start-time">continue</predicate>
                </address>
                <query> select * from wrapper where FILE_COMPLETE = 1 and DEVICE_ID = 300</query>
            </source>
            <query> select * from source </query>
        </stream>
    </streams>
</virtual-sensor>
