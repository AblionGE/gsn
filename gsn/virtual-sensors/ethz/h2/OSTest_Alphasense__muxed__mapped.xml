<virtual-sensor name="OSTest_Alphasense__muxed__mapped" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.BridgeVirtualSensorPermasense</class-name>
        <unique-timestamps>false</unique-timestamps>
        <init-params>
            <param name="position_mapping" />
        </init-params>
        <output-structure>
            <field name="POSITION" type="INTEGER" index="true" />
            <field name="DEVICE_ID" type="INTEGER" />
            <field name="GENERATION_TIME" type="BIGINT" unit="unixtime" />
            <field name="TIMESTAMP" type="BIGINT" unit="unixtime" />
            <field name="MESSAGE_TYPE" type="SMALLINT"/>
            <field name="RAW_DATA" type="VARCHAR(512)"/>
            <field name="MEASUREMENT_ID" type="BIGINT" />
        </output-structure>
    </processing-class>
    <description>Alphasense sensor reading from the OSTest deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage />
    <streams>
        <stream name="data">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="remote-rest">
                    <predicate key="query">select * from OSTest_Alphasense</predicate>
                    <predicate key="remote-contact-point">http://tpvt.permasense.ethz.ch/streaming/</predicate>
                    <predicate key="start-time">continue</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
    </streams>
</virtual-sensor>
