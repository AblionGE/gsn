<virtual-sensor name="OSTest_STEVAL_Dynamic_Raw" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.BridgeVirtualSensorPermasense</class-name>
        <unique-timestamps>false</unique-timestamps>
        <output-structure>
            <field name="DEVICE_ID" type="INTEGER" />
            <field name="GENERATION_TIME" type="BIGINT" unit="unixtime" />
            <field name="TIMESTAMP" type="BIGINT" unit="unixtime" />
            <field name="DURATION" type="DOUBLE" />
            <field name="TIME_OF_DATA" type="DOUBLE" />
            <field name="X_DATA" type="VARCHAR(8192)" />
            <field name="Y_DATA" type="VARCHAR(8192)" />
            <field name="Z_DATA" type="VARCHAR(8192)" />            
        </output-structure>
    </processing-class>
    <description>STEVAL sensor reading from the OSTest deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage />
    <streams>
        
        
        </streams>
</virtual-sensor>
