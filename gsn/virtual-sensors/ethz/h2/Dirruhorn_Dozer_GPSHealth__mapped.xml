<virtual-sensor name="Dirruhorn_Dozer_GPSHealth__mapped" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.BridgeVirtualSensorPermasense</class-name>
        <unique-timestamps>false</unique-timestamps>
        <init-params>
            <param name="position_mapping" />
        </init-params>
        <output-structure>
            <field name="POSITION" type="INTEGER" index="true" />
            <field name="DEVICE_ID" type="INTEGER" />
            <field name="GENERATION_TIME" type="BIGINT" unit="unixtime" index="true"/>
            <field name="TIMESTAMP" type="BIGINT" unit="unixtime"/>
            <field name="HEADER_SEQNR" type="INTEGER" />
            <field name="HEADER_ORIGINATORID" type="INTEGER" />
            <field name="HEADER_ATIME" type="INTEGER" unit="s"/>
            <field name="PAYLOAD_SAMPLE_VALID" type="TINYINT" />
            <field name="PAYLOAD_SAMPLE_NO" type="SMALLINT" />
            <field name="PAYLOAD_UPTIME" type="INTEGER" unit="s"/>
            <field name="PAYLOAD_SYSVOLTAGE" type="INTEGER" />
            <field name="PAYLOAD_TEMPERATURE" type="INTEGER" />
            <field name="PAYLOAD_HUMIDITY" type="INTEGER" />
            <field name="PAYLOAD_NUMCRCERRORS" type="SMALLINT" />
            <field name="PAYLOAD_NUMTIMEOUTS" type="SMALLINT" />
            <field name="PAYLOAD_NUMMEASINTERVALS" type="SMALLINT" />
            <field name="PAYLOAD_NUMPACKETSPERMEASINTERVAL" type="SMALLINT" />
            <field name="PAYLOAD_STARTHOUR" type="SMALLINT" />
            <field name="PAYLOAD_DURATIONHOURS" type="SMALLINT" />
            <field name="PAYLOAD_MEASINTERVAL" type="SMALLINT" />
            <field name="PAYLOAD_MISCSTATUS" type="SMALLINT" />
            <field name="PAYLOAD_QUEUESIZE" type="SMALLINT" />
            <field name="PAYLOAD_PARENTID" type="INTEGER" />
            <field name="PAYLOAD_HOPCOUNT" type="TINYINT" />
            <field name="PAYLOAD_CHILDCOUNT" type="TINYINT" />
        </output-structure>
    </processing-class>
    <description>Dozer messages (DozerGPSHealthMsg) from Dirruhorn deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage />
    <streams>
        <stream name="data">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="remote-rest">
                    <predicate key="query"> select * from Dirruhorn_Dozer_GPSHealth</predicate>
                    <predicate key="remote-contact-point">http://whymper.ethz.ch:22080/streaming/</predicate>
                    <predicate key="start-time">continue</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
    </streams>
</virtual-sensor>
