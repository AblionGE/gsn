<virtual-sensor name="OSTest_OZ47_Calibration_Due" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.OZ47Calibration</class-name>
        <unique-timestamps>false</unique-timestamps>
        <output-structure>
            <field name="POSITION" type="INTEGER" />
            <field name="DEVICE_ID" type="INTEGER" />
            <field name="GENERATION_TIME" type="BIGINT" unit="unixtime" index="true"/>
            
		    		<field name="ADJ_WEIGHT" type="DOUBLE" />
		    		<field name="BIN_SIZE" type="DOUBLE" unit="ppb" />
		    		
		    		<field name="BIN_0_REL_VAL" type="DOUBLE" unit="ppb" />
		    		<field name="BIN_0_SENSOR_VAL" type="DOUBLE" unit="kOhm" />
		    		<field name="BIN_1_REL_VAL" type="DOUBLE" unit="ppb" />
		    		<field name="BIN_1_SENSOR_VAL" type="DOUBLE" unit="kOhm" />
		    		<field name="BIN_2_REL_VAL" type="DOUBLE" unit="ppb" />
		    		<field name="BIN_2_SENSOR_VAL" type="DOUBLE" unit="kOhm" />
		    		<field name="BIN_3_REL_VAL" type="DOUBLE" unit="ppb" />
		    		<field name="BIN_3_SENSOR_VAL" type="DOUBLE" unit="kOhm" />
		    		<field name="BIN_4_REL_VAL" type="DOUBLE" unit="ppb" />
		    		<field name="BIN_4_SENSOR_VAL" type="DOUBLE" unit="kOhm" />
		    		
		    		<field name="CALIB_PARAM_0" type="DOUBLE" />
		    		<field name="CALIB_PARAM_1" type="DOUBLE" />
		    		
            <field name="DATA_IMPORT_SOURCE" type="SMALLINT" />
        </output-structure>
    </processing-class>
    <description>OZ47 calibration parameters for the node in Duebendorf from OSTest deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
   <storage />
    <streams>
        <stream name="data">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="local">
                    <predicate key="query">select * from OSTest_Nabel_Duebendorf__mapped</predicate>
                    <predicate key="start-time">continue</predicate>
                </address>
                <query> select GENERATION_TIME, OZONE_PPB from wrapper where GENERATION_TIME >= 1307450736517 and OZONE_PPB is not NULL</query>
            </source>
            <query> select * from source </query>
        </stream>
    </streams>
</virtual-sensor>
