<virtual-sensor name="OpenSense_OZ47_Static__mapped" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.OZ47VirtualSensor</class-name>
        <unique-timestamps>false</unique-timestamps>
        <init-params>
            <param name="message_type">static</param>
        </init-params>
        <output-structure>
            <field name="POSITION" type="INTEGER" />
            <field name="DEVICE_ID" type="INTEGER" />
            <field name="GENERATION_TIME" type="BIGINT" unit="unixtime" />
            <field name="TIMESTAMP" type="BIGINT" unit="unixtime" />
            <field name="LOT_NO" type="INTEGER"/>
            <field name="CELL_NO" type="INTEGER"/>
            <field name="CALIB_WEEK" type="INTEGER" unit="week"/>
            <field name="CALIB_YEAR" type="INTEGER" unit="year"/>
            <field name="TIMER_PRE_HEAT" type="INTEGER" unit="seconds"/>
            <field name="AUTO_CALIB" type="INTEGER"/>
            <field name="TIMER_DELAY" type="INTEGER" unit="seconds"/>
            <field name="TIMER_CYCLE" type="INTEGER" unit="minutes"/>
            <field name="TIMER_PULSE" type="INTEGER" unit="seconds"/>
            <field name="OFFSET_MEM" type="INTEGER" unit="ppb"/>
            <field name="OFFSET_VERIF" type="INTEGER" unit="ppb"/>
            <field name="OFFSET" type="INTEGER" unit="ppb"/>
            <field name="CALIB_PARAM_X0_1" type="DOUBLE"/>
            <field name="CALIB_PARAM_X1_1" type="DOUBLE"/>
            <field name="CALIB_PARAM_X2_1" type="DOUBLE"/>
            <field name="CALIB_PARAM_X3_1" type="DOUBLE"/>
            <field name="CALIB_PARAM_KT_1" type="DOUBLE"/>
            <field name="CALIB_PARAM_X0_2" type="DOUBLE"/>
            <field name="CALIB_PARAM_X1_2" type="DOUBLE"/>
            <field name="CALIB_PARAM_X2_2" type="DOUBLE"/>
            <field name="CALIB_PARAM_X3_2" type="DOUBLE"/>
            <field name="CALIB_PARAM_KT_2" type="DOUBLE"/>
            <field name="SENSOR_ID" type="INTEGER"/>
            <field name="DATA_IMPORT_SOURCE" type="SMALLINT" />
        </output-structure>
    </processing-class>
    <description>OZ-47 sensor reading from the OpenSense deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage />
    <streams>
        <stream name="data">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="local">
                    <predicate key="query">select * from OpenSense_OZ47__muxed__mapped</predicate>
                    <predicate key="start-time">continue</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
    </streams>
</virtual-sensor>
