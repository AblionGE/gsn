            <field name="Xp_FREQ9" type="DOUBLE" />
            <field name="Xp_DATA10" type="DOUBLE" />
            <field name="Xp_FREQ10" type="DOUBLE" />
            <field name="Yp_DATA1" type="DOUBLE" />
            <field name="Yp_FREQ1" type="DOUBLE" />
            <field name="Yp_DATA2" type="DOUBLE" />
            <field name="Yp_FREQ2" type="DOUBLE" />
            <field name="Yp_DATA3" type="DOUBLE" />
            <field name="Yp_FREQ3" type="DOUBLE" />
            <field name="Yp_DATA4" type="DOUBLE" />
            <field name="Yp_FREQ4" type="DOUBLE" />
            <field name="Yp_DATA5" type="DOUBLE" />
            <field name="Yp_FREQ5" type="DOUBLE" />
