            <field name="Zp_FREQ10" type="DOUBLE" />           
            <field name="DATA_IMPORT_SOURCE" type="SMALLINT" />
        </output-structure>
    </processing-class>
    <description>STEVAL sensor reading from the OpenSense deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage />
    <streams>
        <stream name="data0">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="backlog">
                    <predicate key="remote-connection">opensense-etz-bs01.ethz.ch:9003</predicate>
                    <predicate key="plugin-classname">gsn.wrappers.backlog.plugins.STEVALPlugin</predicate>
                    <predicate key="status-data-type">dynamic_raw_proc</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
        
        <stream name="data1">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="backlog">
                    <predicate key="remote-connection">opensense-caa-etz-2.dyndns.biz:9003</predicate>
                    <predicate key="plugin-classname">gsn.wrappers.backlog.plugins.STEVALPlugin</predicate>
                    <predicate key="status-data-type">dynamic_raw_proc</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
    </streams>
</virtual-sensor>
