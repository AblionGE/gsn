<virtual-sensor name="OpenSense_EMMobile_BT" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.EMMobileDataParserBT</class-name>
        <unique-timestamps>false</unique-timestamps>
        <output-structure>
            <field name="POSITION" type="INTEGER" />
            <field name="DEVICE_ID" type="INTEGER" />
            <field name="GENERATION_TIME" type="BIGINT" unit="unixtime" index="true"/>
            
            <field name="MAC_ADDRESS" type="VARCHAR(256)" />
            <field name="RSSI" type="INTEGER" unit="dBm" />
            <field name="FIELD_STRENGTH" type="DOUBLE" unit="V/m" />
		    
		        <field name="LATITUDE" type="DOUBLE" unit="N"/>
            <field name="LONGITUDE" type="DOUBLE" unit="E"/>
		    
            <field name="DATA_IMPORT_SOURCE" type="SMALLINT" />
        </output-structure>
    </processing-class>
    <description>Android EMMobile BT data from OpenSense deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage storage-directory="/home/perma/permasense_vault/gsn-binaries/" />
    <streams>
        <stream name="data">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="local">
                    <predicate key="query"> select * from OpenSense_Binary__mapped where FILE_COMPLETE = 1 and DEVICE_ID = 301 </predicate>
                    <predicate key="start-time">continue</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
    </streams>
</virtual-sensor>
