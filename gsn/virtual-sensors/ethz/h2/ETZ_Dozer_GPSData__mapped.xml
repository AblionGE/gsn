<virtual-sensor name="ETZ_Dozer_GPSData__mapped" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.BridgeVirtualSensorPermasense</class-name>
        <unique-timestamps>false</unique-timestamps>
        <init-params>
            <param name="position_mapping" />
        </init-params>
        <output-structure>
            <field name="POSITION" type="INTEGER" index="true" />
            <field name="DEVICE_ID" type="INTEGER" />
            <field name="GENERATION_TIME" type="BIGINT" unit="unixtime" index="true"/>
            <field name="TIMESTAMP" type="BIGINT" unit="unixtime"/>
            <field name="HEADER_SEQNR" type="INTEGER" />
            <field name="HEADER_ORIGINATORID" type="INTEGER" />
            <field name="HEADER_ATIME" type="INTEGER" unit="s"/>
	        <field name="PAYLOAD_TIMEOFWEEKMS" type="INTEGER"/>
			<field name="PAYLOAD_WEEKNUMBER" type="INTEGER"/>
      	    <field name="PAYLOAD_CARRIERPHASE_DOUBLE_0" type="SMALLINT"/>
      	    <field name="PAYLOAD_CARRIERPHASE_DOUBLE_1" type="SMALLINT"/>
      	    <field name="PAYLOAD_CARRIERPHASE_DOUBLE_2" type="SMALLINT"/>
      	    <field name="PAYLOAD_CARRIERPHASE_DOUBLE_3" type="SMALLINT"/>
      	    <field name="PAYLOAD_CARRIERPHASE_DOUBLE_4" type="SMALLINT"/>
      	    <field name="PAYLOAD_CARRIERPHASE_DOUBLE_5" type="SMALLINT"/>
      	    <field name="PAYLOAD_CARRIERPHASE_DOUBLE_6" type="SMALLINT"/>
      	    <field name="PAYLOAD_CARRIERPHASE_DOUBLE_7" type="SMALLINT"/>
      	    <field name="PAYLOAD_PSEUDORANGE_DOUBLE_0" type="SMALLINT"/>
      	    <field name="PAYLOAD_PSEUDORANGE_DOUBLE_1" type="SMALLINT"/>
      	    <field name="PAYLOAD_PSEUDORANGE_DOUBLE_2" type="SMALLINT"/>
	        <field name="PAYLOAD_PSEUDORANGE_DOUBLE_3" type="SMALLINT"/>
      	    <field name="PAYLOAD_PSEUDORANGE_DOUBLE_4" type="SMALLINT"/>
	        <field name="PAYLOAD_PSEUDORANGE_DOUBLE_5" type="SMALLINT"/>
      	    <field name="PAYLOAD_PSEUDORANGE_DOUBLE_6" type="SMALLINT"/>
      	    <field name="PAYLOAD_PSEUDORANGE_DOUBLE_7" type="SMALLINT"/>
	        <field name="PAYLOAD_DOPPLER_SINGLE_0" type="SMALLINT"/>
	        <field name="PAYLOAD_DOPPLER_SINGLE_1" type="SMALLINT"/>
      	    <field name="PAYLOAD_DOPPLER_SINGLE_2" type="SMALLINT"/>
      	    <field name="PAYLOAD_DOPPLER_SINGLE_3" type="SMALLINT"/>
      	    <field name="PAYLOAD_VEHICLENUM" type="SMALLINT"/>
	        <field name="PAYLOAD_MEASINDICATOR" type="TINYINT"/>
      	    <field name="PAYLOAD_SIGNALSTRENGTH" type="SMALLINT"/>
      	    <field name="PAYLOAD_LOSSLOCKINDICATOR" type="TINYINT"/>
            <field name="DATA_IMPORT_SOURCE" type="SMALLINT" />
        </output-structure>
    </processing-class>
    <description>Dozer messages (DozerGPSDataMsg) from ETZ deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage />
    <streams>
        <stream name="data">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="remote-rest">
                    <predicate key="query"> select * from ETZ_Dozer_GPSData</predicate>
                    <predicate key="remote-contact-point">http://whymper.ethz.ch:23001/streaming/</predicate>
                    <predicate key="start-time">continue</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
    </streams>
</virtual-sensor>
