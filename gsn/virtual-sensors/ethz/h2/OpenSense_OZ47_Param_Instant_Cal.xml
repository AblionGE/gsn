<virtual-sensor name="OpenSense_OZ47_Param_Instant_Cal" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.OZ47Calibration</class-name>
        <unique-timestamps>false</unique-timestamps>
        <output-structure>
            <field name="POSITION" type="INTEGER" />
            <field name="DEVICE_ID" type="INTEGER" />
            <field name="GENERATION_TIME" type="BIGINT" unit="unixtime" index="true"/>
		    		
		    		<field name="BIN_0_REL_VAL" type="DOUBLE" unit="ppb" />
            <field name="BIN_0_SENSOR_VAL" type="DOUBLE" unit="kOhm" />
            <field name="BIN_0_TIME_VAL" type="BIGINT" unit="unixtime" />
            <field name="BIN_0_AGE_VAL" type="DOUBLE" unit="day" />
            <field name="BIN_1_REL_VAL" type="DOUBLE" unit="ppb" />
            <field name="BIN_1_SENSOR_VAL" type="DOUBLE" unit="kOhm" />
            <field name="BIN_1_TIME_VAL" type="BIGINT" unit="unixtime" />
            <field name="BIN_1_AGE_VAL" type="DOUBLE" unit="day" />
            <field name="BIN_2_REL_VAL" type="DOUBLE" unit="ppb" />
            <field name="BIN_2_SENSOR_VAL" type="DOUBLE" unit="kOhm" />
            <field name="BIN_2_TIME_VAL" type="BIGINT" unit="unixtime" />
            <field name="BIN_2_AGE_VAL" type="DOUBLE" unit="day" />
            <field name="BIN_3_REL_VAL" type="DOUBLE" unit="ppb" />
            <field name="BIN_3_SENSOR_VAL" type="DOUBLE" unit="kOhm" />
            <field name="BIN_3_TIME_VAL" type="BIGINT" unit="unixtime" />
            <field name="BIN_3_AGE_VAL" type="DOUBLE" unit="day" />
            <field name="BIN_4_REL_VAL" type="DOUBLE" unit="ppb" />
            <field name="BIN_4_SENSOR_VAL" type="DOUBLE" unit="kOhm" />
            <field name="BIN_4_TIME_VAL" type="BIGINT" unit="unixtime" />
            <field name="BIN_4_AGE_VAL" type="DOUBLE" unit="day" />
		    		
		    		<field name="CALIB_PARAM_0" type="DOUBLE" />
		    		<field name="CALIB_PARAM_1" type="DOUBLE" />
		    		
            <field name="DATA_IMPORT_SOURCE" type="SMALLINT" />
        </output-structure>
    </processing-class>
    <description>OZ47 instant calibration bin parameters for the OpenSense deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
   <storage />
    <streams>
        <stream name="refdata_stampfenbachstrasse">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="local">
                    <predicate key="query">select * from opensense_ostluft_stampfenbachstrasse</predicate>
                    <predicate key="start-time">continue</predicate>
                </address>
                <query> select GENERATION_TIME, OZONE_PPB from wrapper where OZONE_PPB is not NULL</query>
            </source>
            <query> select * from source </query>
        </stream>
        
        <stream name="refdata_schimmelstrasse">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="local">
                    <predicate key="query">select * from opensense_ostluft_schimmelstrasse</predicate>
                    <predicate key="start-time">continue</predicate>
                </address>
                <query> select GENERATION_TIME, OZONE_PPB from wrapper where OZONE_PPB is not NULL</query>
            </source>
            <query> select * from source </query>
        </stream>
        
        <stream name="sensordata">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="local">
                    <predicate key="query">select * from OpenSense_OZ47_Uncalibrated</predicate>
                    <predicate key="start-time">continue</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
        
    </streams>
</virtual-sensor>
