<virtual-sensor name="OSTest_Alphasense" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.BridgeVirtualSensorPermasense</class-name>
        <unique-timestamps>false</unique-timestamps>
        <output-structure>
            <field name="DEVICE_ID" type="INTEGER" />
            <field name="GENERATION_TIME" type="BIGINT" unit="unixtime" />
            <field name="TIMESTAMP" type="BIGINT" unit="unixtime" />
            <field name="MESSAGE_TYPE" type="SMALLINT"/>
            <field name="RAW_DATA" type="VARCHAR(512)"/>
            <field name="MEASUREMENT_ID" type="BIGINT" />
        </output-structure>
    </processing-class>
    <description>Alphasense sensor reading from the OSTest deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage />
    <streams>
        
        </streams>
</virtual-sensor>
