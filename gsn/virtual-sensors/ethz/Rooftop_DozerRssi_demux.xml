<virtual-sensor name="Rooftop_DozerRssi_4355_demux" priority="10" >
    <processing-class>
       <class-name>gsn.vsensor.BridgeVirtualSensor</class-name>
       <unique-timestamps>false</unique-timestamps>
        <output-structure>
                <field name="GEN_TIME" type="BIGINT" />
                <field name="sequence_number" type="INTEGER" />
                <field name="node_id" type="INTEGER" />
                <field name="rssi_node_id" type="INTEGER" />
                <field name="rssi" type="SMALLINT" />
       </output-structure>
    </processing-class>
    <description>Dozer messages (DozerRssiMsg) from Rooftop deployment in usable form</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage />
    <streams>
    <stream name="data0">
      <source alias="source" storage-size="1" sampling-rate="1">
        <address wrapper="local">
          <predicate key="name">Rooftop_DozerRssi_4355</predicate>
        </address>
        <query> select TIMED, GEN_TIME,
        HEADER_SEQNR as sequence_number,
        HEADER_ORIGINATORID as node_id,
        PAYLOAD_RSSI_ID0 as rssi_node_id,
        PAYLOAD_RSSI_RSSI0 as rssi from wrapper </query>
      </source>
      <query> select * from source where rssi_node_id &lt; 65535</query>
    </stream>
