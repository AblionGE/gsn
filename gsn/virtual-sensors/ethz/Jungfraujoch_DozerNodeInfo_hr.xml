<virtual-sensor name="Jungfraujoch_DozerNodeInfo_hr_3933" priority="10" >
    <processing-class>
       <class-name>gsn.vsensor.BridgeVirtualSensor</class-name>
       <unique-timestamps>true</unique-timestamps>
        <output-structure>
                <field name="generation_time" type="VARCHAR(20)" />
                <field name="sequence_number" type="INTEGER" />
                <field name="node_id" type="INTEGER" />
                <field name="id_0" type="BIGINT" />
                <field name="id_1" type="BIGINT" />
                <field name="xe1205_frequency" type="SMALLINT" />
       </output-structure>
    </processing-class>
    <description>Dozer messages (DozerNodeInfoMsg) from Jungfraujoch deployment in human readable form</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage />
    <streams>
    <stream name="data">
      <source alias="source" storage-size="1" sampling-rate="1">
        <address wrapper="local">
          <predicate key="name">Jungfraujoch_DozerNodeInfo_3933</predicate>
        </address>
        <query> select 	TIMED,
        				timestr(GEN_TIME)							as generation_time,
        				HEADER_SEQNR 								as sequence_number,
        				HEADER_ORIGINATORID 						as node_id,
		                PAYLOAD_ID_LONGARRAY0						as id_0,
		                PAYLOAD_ID_LONGARRAY1						as id_1,
		                PAYLOAD_XE1205FREQUENCY						as xe1205_frequency from wrapper </query>
      </source>
      <query> select * from source</query>
    </stream>
  </streams>
</virtual-sensor>
