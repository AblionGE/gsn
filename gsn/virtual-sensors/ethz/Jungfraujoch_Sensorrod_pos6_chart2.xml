<virtual-sensor name="Jungfraujoch_Sensorrod_pos6_chart2_1396" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.ChartVirtualSensorPermasense</class-name>
        <init-params>
            <param name="input-Stream">data</param>
            <param name="title">Sensorrod resistivity (Position 6, 6d)</param>
            <param name="height">400</param>
            <param name="width">610</param>
            <param name="vertical-axis">MÎ©</param>
            <param name="log-vertical-axis"/>
            <param name="history-size">4320</param>
            <param name="time-stream-name">GENERATIONTIME</param>
        </init-params>
        <output-structure>
            <field name="data" type="binary:image/jpeg"/>
        </output-structure>
    </processing-class>
    <description>Thermistorchain/TC data (merged) from Jungfraujoch deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage history-size="1" />
    <streams>
        <stream name="data">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="local">
                    <predicate key="query"> select GENERATIONTIME, SR_R1, SR_R2, SR_R3, SR_R4, SR_R12, SR_R23, SR_R34, SR_R31, SR_R42, SR_R41 from Jungfraujoch_Sensorrod_1396 where POSITION = 6 and PAYLOAD_SAMPLE_VALID != 0 </predicate>
                    <predicate key="start-time">-518400000</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
    </streams>
</virtual-sensor>
