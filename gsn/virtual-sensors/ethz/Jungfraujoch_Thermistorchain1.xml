<virtual-sensor name="Jungfraujoch_Thermistorchain1_1396" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.BridgeVirtualSensorPermasense</class-name>
        <unique-timestamps>false</unique-timestamps>
        <init-params>
            <param name="sensorvalue_conversion" />
        </init-params>
        <output-structure>
            <field name="TIMESTAMP" type="BIGINT"/>
            <field name="GENERATIONTIME" type="BIGINT"/>
            <field name="POSITION" type="INTEGER" />
            <field name="SENSORTYPE" type="VARCHAR(30)" />
            <field name="SENSORTYPE_SERIALID" type="BIGINT" />
            <field name="HEADER_SEQNR" type="INTEGER" />
            <field name="HEADER_ORIGINATORID" type="INTEGER" />
            <field name="HEADER_ATIME" type="INTEGER" />
            <field name="PAYLOAD_SAMPLE_VALID" type="TINYINT" />
            <field name="PAYLOAD_SAMPLE_NO" type="SMALLINT" />
            <field name="TC_REF1" type="VARCHAR(30)" />
            <field name="TC_REF2" type="VARCHAR(30)" />
            <field name="TC_T1" type="VARCHAR(30)" />
            <field name="TC_T2" type="VARCHAR(30)" />
            <field name="TC_T3" type="VARCHAR(30)" />
            <field name="TC_T4" type="VARCHAR(30)" />
            <field name="TC_REF3" type="VARCHAR(30)" />
            <field name="TC_REF4" type="VARCHAR(30)" />
            <field name="TC_T5" type="VARCHAR(30)" />
        </output-structure>
    </processing-class>
    <description>Thermistorchain/TC data (part 1) from Jungfraujoch deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage />
    <streams>
        <stream name="data">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="local">
                    <predicate key="query"> select * from Jungfraujoch_DozerAdcMux1_mapped_1396 where SENSORTYPE is not null and locate("tc:", SENSORTYPE) != 0 </predicate>
                    <predicate key="start-time">continue</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
    </streams>
</virtual-sensor>
