<virtual-sensor name="Rooftop_DozerNodeHealth_1355" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.BridgeVirtualSensorBackLog</class-name>
        <unique-timestamps>false</unique-timestamps>
        <output-structure>
            <field name="TIMESTAMP" type="BIGINT"/>
            <field name="GENTIME" type="BIGINT"/>
            <field name="HEADER_ATIME_HIGH" type="SMALLINT" />
            <field name="HEADER_ATIME_LOW" type="INTEGER" />
            <field name="HEADER_ORIGINATORID" type="INTEGER" />
            <field name="HEADER_SEQNR" type="INTEGER" />
            <field name="PAYLOAD_FLASHSTATUS" type="INTEGER" />
            <field name="PAYLOAD_HOPCOUNTCHILDCOUNT" type="SMALLINT" />
            <field name="PAYLOAD_HUMIDITY" type="INTEGER" />
            <field name="PAYLOAD_MSPTEMPERATURE" type="INTEGER" />
            <field name="PAYLOAD_SIBCURRENT" type="INTEGER" />
            <field name="PAYLOAD_PARENTID" type="INTEGER" />
            <field name="PAYLOAD_QUEUESIZE" type="SMALLINT" />
            <field name="PAYLOAD_SAMPLENO" type="INTEGER" />
            <field name="PAYLOAD_SDIVOLTAGE" type="INTEGER" />
            <field name="PAYLOAD_SYSVOLTAGE" type="INTEGER" />
            <field name="PAYLOAD_TEMPERATURE" type="INTEGER" />
            <field name="PAYLOAD_UPTIME_HIGH" type="SMALLINT" />
            <field name="PAYLOAD_UPTIME_LOW" type="INTEGER" />
        </output-structure>
    </processing-class>
    <description>Dozer messages (DozerNodeHealthMsg) from ETZ rooftop deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage />
    <streams>
        <stream name="data">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="backlog">
                    <predicate key="deployment">permasense-etzl-bs01.ethz.ch:9003</predicate>
                    <predicate key="plugin-classname">gsn.wrappers.backlog.plugins.MigMessagePlugin</predicate>
                    <predicate key="message-classname">ch.ethz.permafrozer.messages.DozerHealthMsg</predicate>
                    <predicate key="local-sf-port">9006</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source</query>
        </stream>
    </streams>
</virtual-sensor>
