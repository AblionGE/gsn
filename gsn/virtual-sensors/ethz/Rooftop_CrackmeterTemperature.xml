<virtual-sensor name="Rooftop_CrackmeterTemperature_1396" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.BridgeVirtualSensorPermasense</class-name>
        <unique-timestamps>false</unique-timestamps>
        <init-params>
            <param name="sensorvalue_conversion" />
        </init-params>
        <output-structure>
            <field name="TIMESTAMP" type="BIGINT"/>
            <field name="GENERATIONTIME" type="BIGINT"/>
            <field name="POSITION" type="INTEGER" />
            <field name="SENSORTYPE" type="VARCHAR(30)" />
            <field name="SENSORTYPE_SERIALID" type="BIGINT" />
            <field name="HEADER_SEQNR" type="INTEGER" />
            <field name="HEADER_ORIGINATORID" type="INTEGER" />
            <field name="HEADER_ATIME" type="INTEGER" />
            <field name="PAYLOAD_SAMPLE_NO" type="SMALLINT" />
            <field name="CRT_DX1" type="VARCHAR(30)" />
            <field name="CRT_T1" type="VARCHAR(30)" />
            <field name="CRT_T2" type="VARCHAR(30)" />
        </output-structure>
    </processing-class>
    <description>CrackmeterTemperature/CRT data from ETZ rooftop deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage />
    <streams>
        <stream name="data">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="local">
                    <predicate key="query"> select * from Rooftop_DozerAdcComDiff_mapped_1396 where PAYLOAD_SAMPLE_VALID != 0 and SENSORTYPE is not null and locate("crt", SENSORTYPE) != 0 </predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
    </streams>
</virtual-sensor>
