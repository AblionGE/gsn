<virtual-sensor name="Rooftop_DozerDigitalDCX_1376" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.BridgeVirtualSensorBackLog</class-name>
        <unique-timestamps>false</unique-timestamps>
        <output-structure>
            <field name="TIMESTAMP" type="BIGINT"/>
            <field name="GENERATION_TIME" type="BIGINT"/>
            <field name="HEADER_SEQNR" type="INTEGER" />
            <field name="HEADER_ORIGINATORID" type="INTEGER" />
            <field name="HEADER_ATIME" type="INTEGER" />
            <field name="PAYLOAD_SAMPLE_VALID" type="TINYINT" />
            <field name="PAYLOAD_SAMPLE_NO" type="SMALLINT" />
            <field name="PAYLOAD_BATTERYSTATUS" type="SMALLINT" />
            <field name="PAYLOAD_DCXDATA_STATUS0" type="SMALLINT" />
            <field name="PAYLOAD_DCXDATA_STATUS1" type="SMALLINT" />
            <field name="PAYLOAD_DCXDATA_STATUS2" type="SMALLINT" />
            <field name="PAYLOAD_DCXDATA_STATUS3" type="SMALLINT" />
            <field name="PAYLOAD_DCXDATA_FLOATVALUE0" type="DOUBLE" />
            <field name="PAYLOAD_DCXDATA_FLOATVALUE1" type="DOUBLE" />
            <field name="PAYLOAD_DCXDATA_FLOATVALUE2" type="DOUBLE" />
            <field name="PAYLOAD_DCXDATA_FLOATVALUE3" type="DOUBLE" />
        </output-structure>
    </processing-class>
    <description>Dozer messages (DozerDigitalDCXMsg) from ETZ rooftop deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage />
    <streams>
        <stream name="data">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="backlog">
                    <predicate key="plugin-classname">gsn.wrappers.backlog.plugins.MigMessagePlugin</predicate>
                    <predicate key="message-classname">ch.ethz.permadozer.tinyos.DozerDigitalDCXMsg</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
    </streams>
</virtual-sensor>
