<virtual-sensor name="Rooftop_DozerAdcMux2_hr_3933" priority="10" >
    <processing-class>
       <class-name>gsn.vsensor.BridgeVirtualSensor</class-name>
       <unique-timestamps>true</unique-timestamps>
        <output-structure>
                <field name="generation_time" type="VARCHAR(20)" />
                <field name="sequence_number" type="INTEGER" />
                <field name="node_id" type="INTEGER" />
                <field name="valid" type="TINYINT" />
                <field name="sample_number" type="SMALLINT" />
                <field name="sib_adc0_0" type="INTEGER" />
                <field name="sib_adc0_1" type="INTEGER" />
                <field name="sib_adc0_2" type="INTEGER" />
                <field name="sib_adc0_3" type="INTEGER" />
                <field name="sib_adc0_4" type="INTEGER" />
                <field name="sib_adc0_5" type="INTEGER" />
                <field name="sib_adc0_6" type="INTEGER" />
                <field name="sib_adc0_7" type="INTEGER" />
                <field name="sib_adc0_8" type="INTEGER" />
                <field name="sib_adc0_9" type="INTEGER" />
       </output-structure>
    </processing-class>
    <description>Dozer messages (DozerAdcMux2Msg) from ETZ rooftop deployment in human readable form</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage />
    <streams>
    <stream name="data">
      <source alias="source" storage-size="1" sampling-rate="1">
        <address wrapper="local">
          <predicate key="name">Rooftop_DozerAdcMux2_3933</predicate>
        </address>
        <query> select 	TIMED,
        				timestr(GEN_TIME)										as generation_time,
        				HEADER_SEQNR 											as sequence_number,
        				HEADER_ORIGINATORID 									as node_id,
        				to_tinyint(PAYLOAD_SAMPLENO &gt;&gt; 15)				as valid,
        				to_smallint(PAYLOAD_SAMPLENO &amp; 32767)				as sample_number,
		                PAYLOAD_SIBADC00										as sib_adc0_0,
		                PAYLOAD_SIBADC01										as sib_adc0_1,
		                PAYLOAD_SIBADC02										as sib_adc0_2,
		                PAYLOAD_SIBADC03										as sib_adc0_3,
		                PAYLOAD_SIBADC04										as sib_adc0_4,
		                PAYLOAD_SIBADC05										as sib_adc0_5,
		                PAYLOAD_SIBADC06										as sib_adc0_6,
		                PAYLOAD_SIBADC07										as sib_adc0_7,
		                PAYLOAD_SIBADC08										as sib_adc0_8,
		                PAYLOAD_SIBADC09										as sib_adc0_9 from wrapper </query>
      </source>
      <query> select * from source</query>
    </stream>
  </streams>
</virtual-sensor>
