<virtual-sensor name="Rooftop_VaisalaWXT520_1412" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.BridgeVirtualSensorPermasense</class-name>
        <unique-timestamps>false</unique-timestamps>
        <output-structure>
            <field name="TIMESTAMP" type="BIGINT" />
            <field name="Wu" type="VARCHAR(100)" />
            <field name="Tu" type="VARCHAR(100)" />
            <field name="Ru" type="VARCHAR(100)" />
            <field name="Su" type="VARCHAR(100)" />
        </output-structure>
    </processing-class>
    <description>show weather sensor values of Vaisala WXT520 on Basestation from the ETZ rooftop deployment</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage />
    <streams>
        <stream name="data">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="backlog">
                    <predicate key="plugin-classname">gsn.wrappers.backlog.plugins.VaisalaWXT520Plugin</predicate>
                </address>
                <query> select * from wrapper </query>
            </source>
            <query> select * from source</query>
        </stream>
    </streams>
</virtual-sensor>
