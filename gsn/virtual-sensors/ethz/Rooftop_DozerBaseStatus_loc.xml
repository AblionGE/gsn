<virtual-sensor name="Rooftop_DozerBaseStatus_loc_1376" priority="10" >
    <processing-class>
        <class-name>gsn.vsensor.BridgeVirtualSensorBackLog</class-name>
        <unique-timestamps>false</unique-timestamps>
        <init-params>
            <param name="deployment">rooftop</param>
        </init-params>
        <output-structure>
            <field name="TIMESTAMP" type="BIGINT"/>
            <field name="GENERATION_TIME" type="BIGINT"/>
            <field name="LOCATION_ID" type="INTEGER"/>
            <field name="HEADER_SEQNR" type="INTEGER" />
            <field name="HEADER_ORIGINATORID" type="INTEGER" />
            <field name="HEADER_ATIME" type="INTEGER" />
            <field name="PAYLOAD_SAMPLE_VALID" type="TINYINT" />
            <field name="PAYLOAD_SAMPLE_NO" type="SMALLINT" />
            <field name="PAYLOAD_UPTIME" type="INTEGER" />
            <field name="PAYLOAD_TEMPERATURE" type="INTEGER" />
            <field name="PAYLOAD_HUMIDITY" type="INTEGER" />
            <field name="PAYLOAD_MSPVOLTAGE" type="INTEGER" />
            <field name="PAYLOAD_MSPTEMPERATURE" type="INTEGER" />
            <field name="PAYLOAD_QUEUESIZE" type="SMALLINT" />
            <field name="PAYLOAD_PACKETSSENT" type="INTEGER" />
            <field name="PAYLOAD_PACKETSRECEIVED" type="INTEGER" />
            <field name="PAYLOAD_CHILDCOUNT" type="SMALLINT" />
            <field name="PAYLOAD_RSSI" type="SMALLINT" />
        </output-structure>
    </processing-class>
    <description>Dozer messages (DozerBaseStatusMsg) from ETZ rooftop deployment with location id</description>
    <life-cycle pool-size="10" />
    <addressing />
    <storage />
    <streams>
        <stream name="data">
            <source alias="source" storage-size="1" sampling-rate="1">
                <address wrapper="local">
                    <predicate key="query"> select * from Rooftop_DozerBaseStatus_1376 </predicate>
                </address>
                <query> select *, null as LOCATION_ID from wrapper </query>
            </source>
            <query> select * from source </query>
        </stream>
    </streams>
</virtual-sensor>
