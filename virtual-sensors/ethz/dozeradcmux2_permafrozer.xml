<virtual-sensor name="Permafrozer_Dozer_AdcMux2_1410" priority="10">
	<processing-class>
		<class-name>gsn.vsensor.BridgeVirtualSensor</class-name>
		<unique-timestamps>true</unique-timestamps>
		<output-structure>
			<field name="SEQNR" type="DOUBLE" />
			<field name="ORIGINATORID" type="DOUBLE" />

			<field name="ATIME" type="DOUBLE" />
			<field name="GENTIME" type="DOUBLE" />

			<field name="SAMPLENO" type="DOUBLE" />

			<field name="SIBADC0" type="DOUBLE" />
			<field name="SIBADC1" type="DOUBLE" />
			<field name="SIBADC2" type="DOUBLE" />
			<field name="SIBADC3" type="DOUBLE" />
			<field name="SIBADC4" type="DOUBLE" />
			<field name="SIBADC5" type="DOUBLE" />
			<field name="SIBADC6" type="DOUBLE" />
			<field name="SIBADC7" type="DOUBLE" />
			<field name="SIBADC8" type="DOUBLE" />
			<field name="SIBADC9" type="DOUBLE" />

			<field name="TIMESTAMP" type="DOUBLE" />
		</output-structure>
	</processing-class>
	<description>sensor node packets (multiplexed ADC sampling with SIB) from permafrozer WSN</description>
	<life-cycle pool-size="10" />
	<addressing />
	<storage />
	<streams>
		<stream name="data">
			<source alias="source" storage-size="1" sampling-rate="1">
				<address wrapper="ss_tinyos-mig">
					<predicate key="ss-host">localhost</predicate>
					<predicate key="ss-port">25000</predicate>
					<predicate key="continue-on-error">true</predicate>
					<predicate key="wrapper-name">mig2</predicate>
					<predicate key="wrapper-keep-processed-ss-entries">false</predicate>
					<predicate key="source">sf@permafrozer.ethz.ch:9001</predicate>
					<predicate key="message-classname">ch.ethz.permafrozer.DozerAdcMux2Msg</predicate>
					<predicate key="getter-prefix">get</predicate>
				</address>
				<query> select * from wrapper </query>
			</source>
			<query> select * from source</query>
		</stream>
	</streams>
</virtual-sensor>
