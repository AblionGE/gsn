<virtual-sensor name="Whymper_Dozer_AdcComDiff_1410" priority="10">
	<processing-class>
		<class-name>gsn.vsensor.BridgeVirtualSensor</class-name>
		<unique-timestamps>true</unique-timestamps>
		<output-structure>
			<field name="SEQNR" type="DOUBLE" />
			<field name="ORIGINATORID" type="DOUBLE" />

			<field name="ATIME" type="DOUBLE" />
			<field name="GENTIME" type="DOUBLE" />

			<field name="SAMPLENO" type="DOUBLE" />

			<field name="SIBADCDIFF0" type="DOUBLE" />
			<field name="SIBADCDIFF1" type="DOUBLE" />
			<field name="SIBADCDIFF2" type="DOUBLE" />
			<field name="SIBADCDIFF3" type="DOUBLE" />

			<field name="SIBADCCOM0" type="DOUBLE" />
			<field name="SIBADCCOM1" type="DOUBLE" />
			<field name="SIBADCCOM2" type="DOUBLE" />

			<field name="TIMESTAMP" type="DOUBLE" />
		</output-structure>
	</processing-class>
	<description>sensor node packets (differential ADC sampling with SIB board) from whymper WSN</description>
	<life-cycle pool-size="10" />
	<addressing />
	<storage />
	<streams>
		<stream name="data">
			<source alias="source" storage-size="1" sampling-rate="1">
				<address wrapper="ss_tinyos-mig">
					<predicate key="ss-host">localhost</predicate>
					<predicate key="ss-port">25000</predicate>
					<predicate key="continue-on-error">true</predicate>
					<predicate key="wrapper-name">mig2</predicate>
					<predicate key="wrapper-keep-processed-ss-entries">false</predicate>
					<predicate key="source">sf@whymper.ethz.ch:9001</predicate>
					<predicate key="message-classname">ch.ethz.permafrozer.DozerAdcComDiffMsg</predicate>
					<predicate key="getter-prefix">get</predicate>
				</address>
				<query> select * from wrapper </query>
			</source>
			<query> select * from source</query>
		</stream>
	</streams>
</virtual-sensor>
