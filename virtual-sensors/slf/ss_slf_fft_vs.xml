<virtual-sensor name="slf_fft_vs" priority="10" >
    <processing-class>
       <class-name>ch.slf.FFTSample</class-name>
       <init-params>
			<param name="window-size">128</param>
			<param name="step-size">16</param>
		</init-params>
        <output-structure>        
			<field name="TIMESTAMP" type="BIGINT" />
			<field name="DF" type="DOUBLE" />
			<field name="VALS" type="BLOB" />
       </output-structure>
    </processing-class>
    <description>FFT from CSV File Test</description>
    <life-cycle pool-size="10" />
    <addressing/>
    <storage history-size="10"/>
    <streams>
    <stream name="data">
      <source alias="source" storage-size="1" sampling-rate="1">
        <address wrapper="ss_csv">
          <predicate key="ss-host">localhost</predicate>
          <predicate key="ss-port">25000</predicate>
          <predicate key="continue-on-error">true</predicate>
          <predicate key="wrapper-name">csv2</predicate>
          <predicate key="csv-format-file-path">/home/marett/tramm/CR1000_SMALLREL_RADIO_TDR_Wave.dat</predicate>
          <predicate key="csv-skip-lines">5</predicate>
          <predicate key="csv-separator">,</predicate>
          <predicate key="csv-source-file-path">/home/marett/tramm/CR1000_SMALLREL_RADIO_TDR_Wave.dat</predicate>
          <predicate key="csv-format-line-names">2</predicate>
          <predicate key="csv-format-line-descriptions">3,4</predicate>
          <predicate key="csv-format-line-format">5</predicate>
        </address>
        <query> select * from wrapper </query>
      </source>
      <query> select * from source</query>
    </stream>
  </streams>
</virtual-sensor> 
