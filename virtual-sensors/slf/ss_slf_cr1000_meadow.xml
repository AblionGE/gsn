<virtual-sensor name="slf_cr1000_meadows_vs" priority="10" >
    <processing-class>
       <class-name>gsn.vsensor.BridgeVirtualSensor</class-name>
       <unique-timestamps>false</unique-timestamps>
        <output-structure>        
			<field name="TIMESTAMP" type="BIGINT" />
			<field name="RECORD" type="INTEGER" />
			<field name="BATT_VOLT_AVG" type="DOUBLE" />
			<field name="PANEL_TEMP_AVG" type="DOUBLE" />
			<field name="TOPPVWC_1" type="DOUBLE" />
			<field name="TOPPVWC_2" type="DOUBLE" />
			<field name="TOPPVWC_3" type="DOUBLE" />
			<field name="TOPPVWC_4" type="DOUBLE" />
			<field name="TOPPVWC_5" type="DOUBLE" />
			<field name="TOPPVWC_6" type="DOUBLE" />
       </output-structure>
    </processing-class>
    <description>SLF CR1000 MEADOWS VS</description>
    <life-cycle pool-size="10" />
    <addressing/>
    <storage history-size="10"/>
    <streams>
    <stream name="data">
      <source alias="source" storage-size="1" sampling-rate="1">
        <address wrapper="ss_csv">
          <predicate key="ss-host">localhost</predicate>
          <predicate key="ss-port">25000</predicate>
          <predicate key="continue-on-error">true</predicate>
          <predicate key="wrapper-name">csv2</predicate>
          <predicate key="csv-format-file-path">/home/tramm/data/incoming/CR1000_MEADOW_Data_TDR.dat</predicate>
          <predicate key="csv-skip-lines">5</predicate>
          <predicate key="csv-separator">,</predicate>
          <predicate key="csv-source-file-path">/home/tramm/data/incoming/CR1000_MEADOW_Data_TDR.dat</predicate>
          <predicate key="csv-format-line-names">2</predicate>
          <predicate key="csv-format-line-descriptions">3,4</predicate>
          <predicate key="csv-format-line-format">5</predicate>
        </address>
        <query> select * from wrapper </query>
      </source>
      <query> select * from source</query>
    </stream>
  </streams>
</virtual-sensor> 
