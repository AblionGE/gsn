<virtual-sensor name="wannengrat1" priority="10" >    <processing-class>		<class-name>gsn.vsensor.BridgeVirtualSensor</class-name>		   <init-params/>	    <output-structure>			<field name="record" type="double" />			<field name="RH_Avg" type="double" />			<field name="TA_Avg" type="double" />			<field name="VW_WVc1" type="double" />			<field name="VW_WVc2" type="double" />			<field name="VW_WVc3" type="double" />			<field name="VW_WVc4" type="double" />			<field name="VW_Max" type="double" />			<field name="VW_U_Avg" type="double" />			<field name="VW_V_Avg" type="double" />			<field name="VW_W_Avg" type="double" />			<field name="SW_dir_Avg" type="double" />			<field name="SW_ref_Avg" type="double" />			<field name="LW_dir_Avg" type="double" />			<field name="LW_ref_Avg" type="double" />	    </output-structure>    </processing-class>    <description>wannengrat weather station in davos, data provided by SLF.</description>    <life-cycle pool-size="10" />    <addressing>		<predicate key="geographical"></predicate>	</addressing>	<storage/>    <streams>	    <stream name="DATA" >	         <source alias="source" storage-size="1" sampling-rate="1">	            <address wrapper="wannengrat">	               <predicate key="file">Wan_1.met</predicate>	               <predicate key="sampling">600</predicate> <!-- 10 mins, the value is in msc.-->	            </address>	            <query>select * from wrapper</query>	         </source>	         <query>select * from source</query>	    </stream>    </streams></virtual-sensor> 